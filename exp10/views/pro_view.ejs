<html>
    <Head>

    </Head>
    <body>
        <a href="/">Home</a>
        <a href="/about">About</a>
        <a href="/product">Product</a>
        
        <h1>List of All Product</h1>
        <table border="1" align="center" width="700">
            <tr>
                <th>S.No.</th>
                <th>Title</th>
                <th>Price</th>
                <th>Detail</th>
                <th>Delete</th>
                <th>Edit</th>
            </tr>
            <% result.forEach((x, n)=>{ %>
                
                <tr>
                    <td><%= n+1 %></td>
                    <td><%= x.title %></td>
                    <td><%= x.price %></td>
                    <td><a href="/product/detail/<%= x._id %>">Detail</a></td>
                    <td><a href="/product/delete/<%= x._id %>">Delete</a></td>
                    <td><a href="/product/edit/<%= x._id %>">Edit</a></td>
                </tr>
               
            <% }) %>
        </table>
    </body>
</html>