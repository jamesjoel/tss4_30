<%- include('../includes/header') %>
<div class="container my-5" style="min-height: 700px;">
    <div class="row">
        <div class="col-md-12">
            <h3 class="text-center">List All Subject</h3>
            <table class="table table-dark table-bordered table-striped">
                <tr>
                    <th>S.No.</th>
                    <th>Subject Name</th>
                    <th>Delete</th>
                    <th>Edit</th>
                </tr>
                <% result.forEach((x, n)=>{ %>
                    <tr>
                        <td><%= n+1 %></td>
                        <td><%= x.name %></td>
                        <td><a href="/subject/delete/<%= x._id %>" class="btn btn-sm btn-danger">Delete</a></td>
                        <td><a href="/subject/edit/<%= x._id %>" class="btn btn-sm btn-info">Edit</a></td>
                    </tr>
                <% }) %>
            </table>
        </div>
    </div>
</div>
<%- include('../includes/footer') %>